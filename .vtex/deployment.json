[
  {
    "name": "profile-form",
    "description": "",
    "source": "https://github.com/vtex/profile-form.git",
    "script": {
      "deploy": ".vtex/deploy.yml",
      "ci": ".vtex/ci.yml"
    },
   "build": {
      "provider": "dkcicd",
      "pipelines": [
        {
          "name": "node-ci-v2",
          "parameters": {
            "nodeCommands": [ "lint", "build", "test:coverage" ],
            "contextPath": "react"
          },
          "when": [
            {
              "event": "push",
              "source": "branch"
            }
          ]
        }
      ]
    },
    "service": {
      "type": "front"
    },
    "tags": [
      {
        "key": "Product",
        "value": "profile-form"
      },
      {
        "key": "Owner",
        "value": "Post Purchase XP"
      },
      {
        "key": "Team",
        "value": "Post Purchase XP"
      }
    ]
  }
]
